# Supabase Configuration (обязательно)
# Получите эти значения в Supabase Dashboard → Settings → API
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_public_key_here

# Server Configuration
WEBHOOK_PORT=3001
NODE_ENV=development

# Telegram Bot (опционально, для бота)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Google Sheets (опционально, если используете)
GOOGLE_SHEET_ID=your_vacancies_sheet_id_here
GOOGLE_QUESTIONS_SHEET_ID=your_questions_sheet_id_here
GOOGLE_SERVICE_EMAIL=your_service_account_email_here
GOOGLE_PRIVATE_KEY=your_private_key_here

